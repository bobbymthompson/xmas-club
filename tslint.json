{
  "extends": "tslint-ionic-rules",
  "rules": {
    "no-duplicate-variable": true,
    "no-unused-variable": [
      true
    ],
    "eofline": false,
    "typedef-whitespace": [
      false
    ],
    "max-line-length": [
      250
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "no-var-requires": false,
    "no-console": [
      false
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
